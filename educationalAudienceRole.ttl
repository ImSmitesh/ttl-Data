@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix gsord: <https://gsord.germania-sacra.de/resources/> .

<http://uri.gbv.de/terminology/germania_sacra_wiag_orden/>
    a skos:ConceptScheme ;
    skos:prefLabel "Germania Sacra: Wiag Religious Orders"@en ;
    skos:definition "A concept scheme for religious orders used in Germania Sacra and Wiag vocabularies."@en ;
    dct:creator "Germania Sacra / Wiag Project" ;
    dct:language "de" ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:source <https://germania-sacra.de/> ;
    vann:preferredNamespaceUri "https://gsord.germania-sacra.de/resources/" ;
    skos:hasTopConcept gsord:a, gsord:Alexianerinnen, gsord:Kartaeuser .

# Example top concept
gsord:a
    a skos:Concept ;
    skos:prefLabel "Religious Order A"@en ;
    skos:inScheme <http://uri.gbv.de/terminology/germania_sacra_wiag_orden/> .

# Example concept: Alexianerinnen
gsord:Alexianerinnen
    a skos:Concept ;
    skos:prefLabel "Alexianerinnen"@de ;
    skos:broader gsord:a ;
    skos:inScheme <http://uri.gbv.de/terminology/germania_sacra_wiag_orden/> ;
    skos:related gsord:Kartaeuser .

# Example concept: Kartäuser
gsord:Kartaeuser
    a skos:Concept ;
    skos:prefLabel "Kartäuser"@de ;
    skos:broader gsord:a ;
    skos:inScheme <http://uri.gbv.de/terminology/germania_sacra_wiag_orden/> ;
    skos:related gsord:Alexianerinnen .
