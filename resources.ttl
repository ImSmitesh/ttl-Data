# Individual Concepts

gsord:Brudergemeinschaft a skos:Concept;
  skos:prefLabel "Brudergemeinschaft"@de;
  skos:definition "A brotherhood or fraternal religious community."@en;
  skos:inScheme gsord:.

gsord:Franziskanerorden a skos:Concept;
  skos:prefLabel "Franziskanerorden"@de;
  skos:definition "The Franciscan Order founded by St. Francis of Assisi."@en;
  skos:inScheme gsord:.

gsord:Hospitaliterinnen a skos:Concept;
  skos:prefLabel "Hospitaliterinnen"@de;
  skos:definition "A female religious order associated with the Hospitaller tradition."@en;
  skos:inScheme gsord:.

gsord:Semireligiöse_W a skos:Concept;
  skos:prefLabel "Semireligiöse_W"@de;
  skos:definition "A female semi-religious community."@en;
  skos:inScheme gsord:.

gsord:Kreuzherren a skos:Concept;
  skos:prefLabel "Kreuzherren"@de;
  skos:definition "Canons Regular of the Holy Cross."@en;
  skos:inScheme gsord:.

gsord:Orden_nach_der_Benediktregel a skos:Concept;
  skos:prefLabel "Orden nach der Benediktregel"@de;
  skos:definition "Religious orders following the Rule of Saint Benedict."@en;
  skos:inScheme gsord:.

gsord:Orden_nach_der_Augustinusregel a skos:Concept;
  skos:prefLabel "Orden nach der Augustinusregel"@de;
  skos:definition "Religious orders following the Rule of Saint Augustine."@en;
  skos:inScheme gsord:.

gsord:Regularkanoniker a skos:Concept;
  skos:prefLabel "Regularkanoniker"@de;
  skos:definition "Canons Regular, clergy following a specific religious rule."@en;
  skos:inScheme gsord:.

gsord:Semireligiöse_M a skos:Concept;
  skos:prefLabel "Semireligiöse_M"@de;
  skos:definition "A male semi-religious community."@en;
  skos:inScheme gsord:.

gsord:Mendikantenorden a skos:Concept;
  skos:prefLabel "Mendikantenorden"@de;
  skos:definition "Mendicant orders devoted to preaching and poverty."@en;
  skos:inScheme gsord:.

gsord:Hospitaliter a skos:Concept;
  skos:prefLabel "Hospitaliter"@de;
  skos:definition "A male religious order associated with the Hospitaller tradition."@en;
  skos:inScheme gsord:.

gsord:Kapuzinerinnen a skos:Concept;
  skos:prefLabel "Kapuzinerinnen"@de;
  skos:definition "The female branch of the Capuchin order."@en;
  skos:inScheme gsord:.

gsord:Barmherzige_Brueder a skos:Concept;
  skos:prefLabel "Barmherzige Brüder"@de;
  skos:definition "The Brothers of Mercy, a religious congregation caring for the sick."@en;
  skos:inScheme gsord:.

# Example from your previous data
gsord:Coelestiner a skos:Concept;
  skos:prefLabel "Coelestiner"@de;
  skos:definition "A monastic congregation founded by Pope Coelestin V following the Benedictine rule."@en;
  skos:broader gsord:Orden_nach_der_Benediktregel;
  skos:inScheme gsord:.

gsord:Kartäuser a skos:Concept;
  skos:prefLabel "Kartäuser"@de;
  skos:definition "The Carthusian Order, a strict monastic order following the Benedictine rule."@en;
  skos:broader gsord:Orden_nach_der_Benediktregel;
  skos:inScheme gsord:.

gsord:Alexianerinnen a skos:Concept;
  skos:prefLabel "Alexianerinnen"@de;
  skos:definition "A female religious order from the late Middle Ages, following the Augustinian rule."@en;
  skos:broader gsord:Orden_nach_der_Augustinusregel;
  skos:inScheme gsord:.
